{
    "name": "ulasoft/pay-uz",
    "description": "Payment: Click, Payme, Uzcard, Visa",
    "keywords": [
        "ulasoft",
        "pay-uz"
    ],
    "homepage": "https://github.com/ulasoft/pay-uz",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shaxzodbek",
            "email": "shaxzodbek.qambaraliyev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.*",
        "illuminate/support": "5.7.*|5.8.*|6.*|7.*|8.*",
        "stripe/stripe-php": "^6.31"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Goodoneuz\\PayUz\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goodoneuz\\PayUz\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goodoneuz\\PayUz\\PayUzServiceProvider"
            ],
            "aliases": {
                "PayUz": "Goodoneuz\\PayUz\\PayUzFacade"
            }
        }
    }
}
